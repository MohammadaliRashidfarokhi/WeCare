@model IQueryable<PCR.Models.ViewModels.SamplesPdf>
@{
    Layout = "_ManageLayout";

}


@section ul{
  <ul class="navbar-nav ml-auto">
      <li class="nav-item"><a class="nav-link" asp-action="Index" asp-controller="Doctor">All Reports</a></li>
      <li class="nav-item"><a class="nav-link" asp-action="Index" asp-controller="Home">Home</a></li>
      
      <li class="nav-item">  <a class="nav-link" href='javascript:history.go(-1)'>Back</a></li>

  </ul>


  <form class="form-inline">

    <button asp-controller="Account" asp-action="Logout" class="btn btn-outline-success my-2 my-sm-0" type="submit">Log out</button>
  </form>

}

<form  asp-action="AddResult">
  <table class="table">
    <thead class="thead-dark">
    <tr>


      <th scope="col">Reportid</th>
      <th scope="col">Patient</th>
      <th scope="col">Pass number</th>
      <th scope="col">Add Result</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
      <tr>
        <td>@user.Reportid</td>
        <td>@user.Patient</td>
        <td>@user.PassportNumber</td>

      
        <td>
        
   
        <a name="Reportid" asp-route-id=@user.Reportid asp-action="PatientInfo">Add Result</a>
     
   
        </td>

      </tr>

    }
    </tbody>
  </table>
</form>